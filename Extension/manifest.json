{
  "version": "0.0.104",
  "name": "Browser Hijacker with Extension",
  "description": "Display on new tab",
  "chrome_url_overrides":{
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs", 
    "bookmarks"
  ],
  "host_permissions": ["*://*/*"],
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["*://*/*"]
    }
  ],
  "background": {
    "service_worker": "script.js"
  },
  "manifest_version": 3
}